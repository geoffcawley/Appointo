			bat 'msbuild Appointo.Entity.csproj'
		} catch(e) {
			slackSend message:'Appointo build failure' color:'danger'
			throw e
		}
    }

    stage('analyze') {
		try {
		} catch(e) {
			throw e
		}
    }

    stage('package') {
		try {
		} catch(e) {
			throw e
		}
    }
    
    stage('deploy') {
		try {
		} catch(e) {
			throw e
		}
    }
}