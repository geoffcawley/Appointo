node('master'){
    stage('import'){
		try {
			git 'https://github.com/geoffcawley/Appointo.git'
		} catch(e) {
			slackSend(message:'Appointo failed import')
			throw e
		}
    }   
    
    stage('build') {
		try {
			dir('C:\\Program Files (x86)\\Jenkins\\workspace\\appointo-pipe\\Appointo.Entity')	//location of solution file
			bat 'nuget restore'
			bat 'msbuild /t:clean,Appointo.Entity'
		} catch(e) {
			slackSend(message:'Appointo build failure')
			throw e
		}
    }

    stage('analyze') {
		try {
		} catch(e) {
			throw e
		}
    }

    stage('package') {
		try {
		} catch(e) {
			throw e
		}
    }
    
    stage('deploy') {
		try {
		} catch(e) {
			throw e
		}
    }
}